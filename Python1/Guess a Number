import random
number = random.randint(1, 100)
pick = 0

count = 0
while pick != number:
  pick = int(input("Pick a number? "))
  count += 1

  if pick < number:
    print("Too low\n")
    
  elif pick > number:
    print("Too high\n")
    
  else:
    print("You win!\n")
    print("count =", count)
